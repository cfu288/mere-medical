{
  "name": "fhir-oauth",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/fhir-oauth/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/fhir-oauth",
        "main": "libs/fhir-oauth/src/index.ts",
        "tsConfig": "libs/fhir-oauth/tsconfig.lib.json",
        "assets": ["libs/fhir-oauth/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/fhir-oauth/jest.config.ts"
      }
    }
  }
}
